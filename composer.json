{
    "name": "laravel/laravel",
    "description": "Expenses, Invoices and Project Manager CRM built on Laravel",
    "keywords": [
        "invoice",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "William Mandai",
            "email": "wm@gitbench.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "2checkout/2checkout-php": "^0.3.1",
        "anandsiddharth/laravel-paytm-wallet": "^1.0",
        "arrilot/laravel-widgets": "^3.7",
        "bacon/bacon-qr-code": "^1.0.3",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "bepsvpt/secure-headers": "^5.2",
        "braintree/braintree_php": "^3.38",
        "cartalyst/stripe-laravel": "^9.0",
        "ddeboer/imap": "^1.0",
        "eluceo/ical": "^0.11",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "hautelook/phpass": "^1.1",
        "jenssegers/date": "^3.5",
        "kwn/number-to-words": "^1.3",
        "laravel/cashier": "^9.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/socialite": "^4.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.5",
        "laravolt/avatar": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/html-to-markdown": "^4.4",
        "maatwebsite/excel": "^3.0",
        "mollie/laravel-mollie": "^2.0",
        "nwidart/laravel-modules": "^4.0",
        "oriceon/oauth-5-laravel": "dev-master",
        "pragmarx/google2fa-laravel": "^1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.2",
        "razorpay/razorpay": "^2.3",
        "renatomarinho/laravel-page-speed": "^1.8",
        "sebastian-berc/repositories": "^1.0",
        "sentry/sentry-laravel": "^1.0",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-backup": "^5.0",
        "spatie/laravel-permission": "^2.4",
        "spatie/laravel-translation-loader": "^2.2",
        "srmklive/paypal": "^1.5",
        "symfony/dom-crawler": "^4.1",
        "wepay/php-sdk": "^0.3.1",
        "yajra/laravel-datatables-oracle": "^8.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "*",
        "deployer/deployer": "^6.3",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "nunomaduro/larastan": "^0.3.4",
        "phpunit/phpunit": "~7.0",
        "squizlabs/php_codesniffer": "^3.3",
        "beyondcode/laravel-self-diagnosis": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helpers/App.php",
            "app/Helpers/DateHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
